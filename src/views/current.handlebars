<h1>{{title}}</h1>

<form class='form-insert' action='' method='POST' enctype='multipart/form-data'>
	<label for='name'>Name</label>
	<input type='text' name='name' id='name' />
	<label for='Surname'>Surname</label>
	<input type='text' name='surname' id='surname' />
	<div class='items-group'>
		<div>
			<label for='age'>Age</label>
			<input type='number' name='age' id='age' />

		</div>
		<div>
			<label for='email'>Email</label>
			<input type='email' name='email' id='email' />

		</div>

	</div>

	<div class='container-buttons j-end'>
		<button type='button' onclick="window.location.href = '/'">Home</button>
	</div>
</form>

<script type='module' src='/js/current.js'></script>