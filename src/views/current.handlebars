<h1>{{title}}</h1>
<div id='main-content'>

	<form class='form-insert' action='' method='POST' enctype='multipart/form-data'>
		<label for='name'>Name</label>
		<input type='text' name='name' id='name' readonly />
		<label for='Surname'>Surname</label>
		<input type='text' name='surname' id='surname' readonly />
		<div class='items-group'>
			<div>
				<label for='age'>Age</label>
				<input type='number' name='age' id='age' readonly />

			</div>
			<div>
				<label for='email'>Email</label>
				<input type='email' name='email' id='email' readonly />

			</div>
			<div>
				<label for='role'>Role</label>
				<input type='text' name='role' id='role' readonly />
			</div>
		</div>

		<div class='container-buttons j-end'>
			<button type='button' onclick="window.location.href = '/'">Home</button>
		</div>
	</form>
</div>

<script type='module' src='/js/current.js'></script>